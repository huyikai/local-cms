{
  "name": "local-cms",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "run-p type-check build-only",
    "build-only": "vite build --emptyOutDir",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "pre-commit": "run-p lint type-check",
    "preview": "vite preview",
    "test": "vitest --coverage --environment jsdom",
    "test:ui": "vitest --ui",
    "test:unit": "vitest --environment jsdom --root src/",
    "type-check": "vue-tsc --composite false --noEmit -p tsconfig.vitest.json"
  },
  "dependencies": {
    "@bytemd/plugin-gfm": "^1.21.0",
    "@bytemd/vue-next": "^1.21.0",
    "@mdit-vue/plugin-component": "^2.0.0",
    "@mdit-vue/plugin-frontmatter": "^2.0.0",
    "@mdit-vue/plugin-headers": "^2.0.0",
    "@mdit-vue/plugin-sfc": "^2.0.0",
    "@mdit-vue/plugin-title": "^2.0.0",
    "@mdit-vue/plugin-toc": "^2.0.0",
    "@vueuse/core": "^10.2.0",
    "ant-design-vue": "^4.1.2",
    "axios": "^1.3.4",
    "bytemd": "^1.21.0",
    "markdown-it": "^14.0.0",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-container": "^4.0.0",
    "markdown-it-emoji": "^3.0.0",
    "pinia": "^2.0.28",
    "tree-conver": "^0.0.93",
    "uuid": "^9.0.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@nolebase/markdown-it-element-transform": "^1.20.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/markdown-it": "^13.0.7",
    "@types/markdown-it-anchor": "^7.0.0",
    "@types/markdown-it-attrs": "^4.1.3",
    "@types/markdown-it-container": "^2.0.9",
    "@types/markdown-it-emoji": "^2.0.4",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-v8": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.63.3",
    "typescript": "~4.9.5",
    "unplugin-auto-import": "^0.14.4",
    "unplugin-vue-components": "^0.24.0",
    "vite": "^4.0.0",
    "vitepress": "^1.0.0-rc.45",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.0.12"
  }
}
